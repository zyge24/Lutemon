<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MoveLutemon">

    <Spinner
        android:id="@+id/sLutemon"
        android:layout_width="199dp"
        android:layout_height="41dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/sLocation"
        android:layout_width="199dp"
        android:layout_height="41dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sLutemon" />

    <Button
        android:id="@+id/btnMoveLutemon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="72dp"
        android:text="SiirrÃ¤ Lutemon"
        app:layout_constraintEnd_toEndOf="@+id/sLocation"
        app:layout_constraintStart_toStartOf="@+id/sLocation"
        app:layout_constraintTop_toBottomOf="@+id/sLocation" />
</androidx.constraintlayout.widget.ConstraintLayout>